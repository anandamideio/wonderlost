{
  "id": "wonderlost",
  "title": "Wonderlost",
  "version": "#{VERSION}#",
  "description": "A FVTT module to add features from the Wondershade system to the PF2e game system, as well as adding in features I think are missing Foundry.",
  "esmodules": [
    "greensock/esm/all.js",
    "dist/wonderlost.mjs"
  ],
  "scripts": [],
  "styles": [
    "styles/module.css"
  ],
  "type": "module",
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "authors": [
    {
      "name": "ModernCannabist",
      "discord": "ModernCannabist",
      "github": "Abourass",
      "flags": {}
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
